"Raylib + ImGui Docking Boilerplate": {
		"prefix": "Docking",
		"body": [
			"#include <rlImGui.h>",
			"#include <imgui.h>",
			"#include \"ImGuiCustomTheme.h\"\n",
			"int main()",
			"{",
			"\tInitWindow(800, 600, \"raylib + ImGui Docking Example\");",
			"\trlImGuiSetup(true);\n",
			"\t// Enable docking",
			"\tImGuiIO &io = ImGui::GetIO();",
			"\tio.ConfigFlags |= ImGuiConfigFlags_DockingEnable;",
			"\tImGuiViewport *viewport = ImGui::GetMainViewport();\n",
			"\tImCustomTheme();",
			"\tSetTargetFPS(60);",
			"\twhile (!WindowShouldClose())",
			"\t{",
			"\t\tBeginDrawing();",
			"\t\tClearBackground(DARKGRAY);",
			"\t\trlImGuiBegin();",
			"\t\tImGui::DockSpaceOverViewport(viewport->ID);",
			"\t\tImGui::Begin(\"Debug\");\n",
			"\t\t$1\n",
			"\t\tImGui::End();",
			"\t\trlImGuiEnd();",
			"\t\tEndDrawing();",
			"\t}\n",
			"\trlImGuiShutdown();",
			"\tCloseWindow();",
			"\treturn 0;",
			"}"
		],
		"description": "Raylib + ImGui Docking Boilerplate with Tab Indentation"
	}
