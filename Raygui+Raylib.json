			"#include <raygui.h>\n",
			"int main()",
			"{",
			"\tInitWindow(800, 600, \" \");\n",
			"\tSetTargetFPS(60);",
			"\twhile (!WindowShouldClose())",
			"\t{",
			"\t\tBeginDrawing();",
			"\t\tClearBackground(BLACK);\n", 
			"\t\t$1\n",
			"\t\tEndDrawing();",
			"\t}\n",
			"\tCloseWindow();",
			"\treturn 0;",
			"}"
