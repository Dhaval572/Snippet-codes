"Imgui lib": {
    "prefix": "Dear imgui",
    "body": [
      "#include <rlImGui.h>",
      "#include <imgui.h>",
      "",
      "int main()",
      "{",
      "\tInitWindow(800, 600, \"raylib + ImGui Example\");",
      "\t",
      "\tSetTargetFPS(60);",
      "\trlImGuiSetup(true);",
      "\twhile (!WindowShouldClose())",
      "\t{",
      "\t\tBeginDrawing();",
      "\t\tClearBackground(DARKGRAY);",
      "\t\trlImGuiBegin();",
      "\t\t",
	  "\t\t$1",
      "\t\t",
      "\t\trlImGuiEnd();",
      "\t\tEndDrawing();",
      "\t}",
      "\t",
      "\trlImGuiShutdown();",
      "\tCloseWindow();",
      "\treturn 0;",
      "}"
    ],
  }
